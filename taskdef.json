{ 
   "executionRoleArn":"arn:aws:iam::058264351046:role/ecsTaskExecutionRole",
   "containerDefinitions":[ 
      { 
         "name":"my-apache",
         "image":"<IMAGE1_NAME>",
         "essential":true,
         "portMappings":[ 
            { 
               "hostPort":80,
               "protocol":"tcp",
               "containerPort":80
            }
         ]
      }
   ],
   "requiresCompatibilities":[ 
      "EC2",
      "FARGATE"
   ],
   "networkMode":"awsvpc",
   "cpu":"256",
   "memory":"512",
   "family":"my-apache"
}

